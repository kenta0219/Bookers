<div class="container">
	<div class="row">
		<div class="col-lg-3">
			<%= render 'books/list', books: @books, book: @book, user: current_user %>
		</div>

						<div class="col-lg-9">
							<table class="table table-striped">
							<h1>Users</h1>
								<thead>
									<tr>
										<th>image</th>
										<th>name</th>
									</tr>

									<% @users.each do |user| %>
										<tr>
											<td> <%= attachment_image_tag user,:profile_image,class: "img-responsive pull-left profile-thumb", size:'60x60'%></td>
											<td><%= user.name %></td>
											<td><%= link_to 'show',user_path(user.id) %></td>
										</tr>
									<% end %>

								</thead>
								<tbody>
									<tr>
										<%#= link_to 'show' %>
									</tr>
								</tbody>
							</table>
						</div>


	</div>
</div>